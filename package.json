{
  "name": "eyes-cypress-cucumber-examples",
  "version": "1.0.0",
  "description": "Example Applitools Eyes Visual UI tests for Cypress and Cucumber",
  "main": "index.js",
  "scripts": {
    "open": "cypress open --e2e --browser '/Applications/Google Chrome.app/Contents/MacOS/Google Chrome'",
    "test": "cypress run"
  },
  "keywords": [
    "applitools",
    "eyes",
    "cypress",
    "cucumber",
    "testing",
    "example"
  ],
  "author": "Sebastian Bernheim",
  "license": "Apache-2.0",
  "dependencies": {
    "@applitools/eyes-cypress": "^3.29.1",
    "@badeball/cypress-cucumber-preprocessor": "^15.1.4",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "cypress": "^12.7.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": [
      "cypress/e2e/step-definitions/**/*.cy.js"
    ]
  }
}
